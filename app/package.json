    "styled-components": "^5.2.3",
    "ts-node": "^10.4.0",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^2.0.2",
    "cypress": "^9.2.0",
    "nodemon": "2.0.0"
  },
  "packageManager": "yarn@1.22.17"
}
