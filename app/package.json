{
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' ts-node --project tsconfig.server.json server.ts",
    "build": "yarn run build:server && yarn build:next",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:schema": "postgraphile -X -c postgres://localhost/connectivity_intake -s connectivity_intake_public --export-schema-graphql dbSchema/schema.graphql --export-schema-json dbSchema/schema.json --classic-ids --sort-export && echo >> dbSchema/schema.json",
    "start": "node --unhandled-rejections=strict dist/server.js",
    "cypress:open": "yarn dev & wait-on http:localhost:3000 & cypress open"
  },
  "dependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-typescript": "^7.16.7",
    "@bcgov-cas/sso-express": "^2.1.0",
    "@button-inc/bcgov-theme": "^1.0.1",
    "@button-inc/form-schema": "^1.0.0-alpha.8",
    "@graphile-contrib/pg-many-to-many": "^1.0.1",
    "@graphile-contrib/pg-omit-archived": "^2.1.0",
    "@graphile-contrib/pg-order-by-related": "^1.0.0-beta.6",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@rjsf/core": "^3.2.1",
    "@rjsf/semantic-ui": "^3.2.1",
    "@types/express": "4.17.2",
    "axios": "^0.25.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "body-parser": "^1.19.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "compression": "^1.7.4",
    "connect-pg-simple": "^7.0.0",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.3",
    "delay": "^5.0.0",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "helmet": "^5.0.1",
    "lightship": "^6.8.0",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "morgan": "^1.10.0",
    "next": "latest",
    "next-session": "^3.4.0",
    "openid-client": "^5.1.2",
    "pg": "^8.7.1",
    "postgraphile": "^4.12.8",
    "postgraphile-log-consola": "^1.0.1",
    "postgraphile-plugin-connection-filter": "^2.2.2",
